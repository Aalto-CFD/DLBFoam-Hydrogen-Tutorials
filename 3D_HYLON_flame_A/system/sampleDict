FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type sets;
libs            ("libsampling.so");

interpolationScheme cellPoint;

setFormat       csv;

sets
(
    data2mm
    {
        type    lineUniform;
        axis    x;
        start   (-0.026 0 0.037129);
        end     (0.026 0 0.037129);
        nPoints 1000;
    }
    
        data5mm
    {
        type    lineUniform;
        axis    x;
        start   (-0.026 0 0.039796);
        end     (0.026 0 0.039796);
        nPoints 1000;
    }
    
        data10mm
    {
        type    lineUniform;
        axis    x;
        start   (-0.026 0 0.045036);
        end     (0.026 0 0.045036);
        nPoints 1000;
    }
    
        data15mm
    {
        type    lineUniform;
        axis    x;
        start   (-0.026 0 0.050097);
        end     (0.026 0 0.050097);
