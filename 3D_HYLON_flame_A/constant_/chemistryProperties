FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    solver          ode_pyJac;
    method          loadBalanced_pyJac;
}

loadbalancing
{
    active true;
    log	false;
}

refmapping
{
    active  true;

    mixtureFractionProperties
    {

        oxidizerMassFractions // air coflow (humid air)
        {
            N2 0.77;
            O2 0.23;
      
        }

        fuelMassFractions 
        {
            H2  1;
        }

    #include "/scratch/project_2004667/shahinzi/DLBmech/Capurso/foam/thermo.foam"



    }

    tolerance 1e-4;
    deltaT 1;
}


chemistry       on;

initialChemicalTimeStep 1;




odeCoeffs
{
    solver          seulex_LAPACK;
    absTol          1e-08;
    relTol          1e-05;
}

reactions
{
}

