FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/gas";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101000;

boundaryField
{
    "(inletMain|inletAir|side|outlet|gas_to_nozzle)"
    {
        type            calculated;
        value           $internalField;
    }

    "frontAndBack.*"
    {
        type            wedge;
    }

    axis
    {
        type            empty;
    }
}


// ************************************************************************* //
