FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/gas";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    inletMain
    {
        type            fixedValue;
        value           $internalField;
    }

    inletAir
    {
        type            fixedValue;
        value           $internalField;
    }

    side
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
    }
    
    gas_to_nozzle
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            T;
    }

    "frontAndBack.*"
    {
        type            wedge;
    }

    axis
    {
        type            empty;
    }

}


// ************************************************************************* //
