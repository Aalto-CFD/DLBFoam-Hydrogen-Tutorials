FoamFile
{
    version         2;
    format          ascii;
    class           volScalarField;
    location        "0";
    object          H2O2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 0 0 ];

#include        "planarMappingDict"

#include        "readInletValue.H"

#include        "readInternalScalarField.H"

internalField   nonuniform $internalScalarField;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform $inletValue;
    }
    outlet
    {
        type            zeroGradient;
    }
    "top|bottom"
    {
        type            cyclic;
    }
    empty
    {
        type            empty;
    }
}


