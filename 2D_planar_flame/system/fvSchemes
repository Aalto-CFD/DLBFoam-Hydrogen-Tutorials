FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         backward;
}

gradSchemes
{
    default         Gauss cubic;
}

divSchemes
{
    default         none;
    div(phi,U)      Gauss limitedCubicV 0;
    div(phi,Yi_h)   Gauss limitedCubic  0;
    div(phi,K)      Gauss limitedCubic  0;
    div(phi,h)      Gauss limitedCubic  0;
    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss cubic;

}

laplacianSchemes
{
    default         Gauss cubic orthogonal;
}

interpolationSchemes
{
    default         cubic;
}

snGradSchemes
{
    default         orthogonal;
}


// ************************************************************************* //
